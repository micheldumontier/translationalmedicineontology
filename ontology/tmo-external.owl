  <owl:Class rdf:about="http://www.ifomis.org/bfo/1.1/span#ScatteredSpatiotemporalRegion">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >scattered_spatiotemporal_region</rdfs:label>
